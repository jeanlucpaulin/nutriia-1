<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="72dp"
    android:background="@color/white"
    android:elevation="15dp"
    android:gravity="center_horizontal">

    <LinearLayout
        android:id="@+id/navbar_target"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginEnd="20dp">

        <ImageView
            android:id="@+id/navbar_target_image"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/menu_icon_target_outline"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            />
        <TextView
            android:id="@+id/navbar_target_text"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/menu_target"
            android:fontFamily="@font/montserrat"
            android:textSize="10sp"
            android:gravity="center"
            android:textColor="@color/black"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/navbar_analysis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginEnd="10dp">

        <ImageView
            android:id="@+id/navbar_analysis_image"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/menu_icon_leaves_outline"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            />

        <TextView
            android:id="@+id/navbar_analysis_text"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/menu_analysis"
            android:fontFamily="@font/montserrat"
            android:textColor="@color/black"
            android:textSize="10sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/navbar_meet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginStart="10dp">

        <ImageView
            android:id="@+id/navbar_meet_image"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/menu_icon_chat_advisor_outline"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            />
        <TextView
            android:id="@+id/navbar_meet_text"
            android:layout_width="70dp"
            android:layout_marginTop="5dp"
            android:layout_height="wrap_content"
            android:text="@string/menu_meet"
            android:fontFamily="@font/montserrat"
            android:textSize="10sp"
            android:gravity="center"
            android:textColor="@color/black"
            />
    </LinearLayout>
